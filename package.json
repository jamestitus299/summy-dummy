{
  "private": true,
  "name": "react-code-canvas",
  "version": "4.0.1",
  "type": "module",
  "sideEffects": false,
  "description": "React component for running and previewing React code with live editing capabilities",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "echo 'Dependencies installed'",
    "size": "size-limit",
