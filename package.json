    "clean": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --write '**/src/**/*.{ts,tsx}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
